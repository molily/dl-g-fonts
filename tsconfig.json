{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "2.0.0",
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true
  }
}